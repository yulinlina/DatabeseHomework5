<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>SelectServlet</servlet-name>
    <servlet-class>com.controller.library.SelectServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>SelectBynameServlet</servlet-name>
    <servlet-class>com.controller.library.SelectBynameServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>SelectByhobbyServlet</servlet-name>
    <servlet-class>com.controller.library.SelectByhobbyServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>SelectServlet</servlet-name>
    <url-pattern>/servlet/SelectServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SelectBynameServlet</servlet-name>
    <url-pattern>/servlet/SelectBynameServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SelectByhobbyServlet</servlet-name>
    <url-pattern>/servlet/SelectByhobbyServlet</url-pattern>
  </servlet-mapping>

</web-app>
